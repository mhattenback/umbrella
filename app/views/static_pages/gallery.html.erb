
<% provide(:title, 'Gallery') %>

<h1>Gallery</h1>
<p>
  <div id="blueimp-gallery" class="blueimp-gallery">
    <div class="slides"></div>
    <h3 class="title"></h3>
    <a class="prev">‹</a>
    <a class="next">›</a>
    <a class="close">×</a>
    <a class="play-pause"></a>
    <ol class="indicator"></ol>
  </div>
</p>

<div id="links">
  <a href="gallery/photo1.jpg" title="Photo 1">
    <img src="gallery/thumb_photo1.jpg" alt="Photo 1">
  </a>
  <a href="gallery/photo2.jpg" title="Photo 2">
    <img src="gallery/thumb_photo2.jpg" alt="Photo 2">
  </a>
  <a href="gallery/photo3.jpg" title="Photo 3">
    <img src="gallery/thumb_photo3.jpg" alt="Photo 3">
  </a>
</div>


